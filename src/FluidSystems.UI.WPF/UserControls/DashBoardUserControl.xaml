<UserControl
    x:Class="FluidSystems.UI.WPF.UserControls.DashBoardUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:FluidSystems.UI.WPF.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:uc="clr-namespace:FluidSystems.UI.WPF.UserControls"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid MinWidth="1000">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <uc:DiagramControl
            Width="800"
            Height="600"
            Margin="4"
            DataContext="{Binding DiagramContent}" />
        <local:ComponentOverviewUserControl Grid.Column="1" DataContext="{Binding ComponentOverviewViewModel}" />
    </Grid>
</UserControl>
